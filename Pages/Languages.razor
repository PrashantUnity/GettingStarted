@page "/Languages"
@page "/Language"
<PageTitle>Language Dimension</PageTitle>
<MudPaper Class="ma-6">
    <MudExpansionPanels>
        @foreach (var component in list)
        {
            <MudExpansionPanel Text="@component.Name">
                <MudGrid>
                    <MudItem xs="12">
                        <MudTabs Elevation="4" Rounded="true" Centered="true" Style="@($"color:{Theme.Palette.SuccessLighten}; background:{Theme.Palette.Dark.ColorDarken};")">
                            <MudTabPanel Text="C#">
                                @RenderComponent(component.Sharp)
                            </MudTabPanel>
                            <MudTabPanel Text="Python">
                                @RenderComponent(component.Python)
                            </MudTabPanel>
                            <MudTabPanel Text="Golang">
                                @RenderComponent(component.Golang)
                            </MudTabPanel>
                            <MudTabPanel Text="Rust">
                                @RenderComponent(component.Rust)
                            </MudTabPanel>
                            <MudTabPanel Text="C++">
                                @RenderComponent(component.Plus)
                            </MudTabPanel>
                        </MudTabs>
                    </MudItem>
                </MudGrid>
            </MudExpansionPanel>
        }
    </MudExpansionPanels>
</MudPaper>

@code {
    private MudTheme Theme = new MudTheme();
    List<(
        ComponentBase Rust,
        ComponentBase Golang,
        ComponentBase Sharp,
        ComponentBase Python,
        ComponentBase Plus,

        string Name)> list = new()
        {
        (new CommentRust(),new CommentGo(),new CommentSharp(), new CommentPython(), new CommentPlus(),"Commenting"),
        (new WriteRust(),new WriteGo(),new WriteSharp(), new WritePython(), new WritePlus(),"Write to Console"),
        (new PrimitiveRust(),new PrimitiveGo(),new PrimitiveSharp(), new PrimitivePython(), new PrimitivePlus(),"Primitive Data Types"),
        (new VariableRust(),new VariableGo(),new VariableSharp(), new VariablePython(), new VariablePlus(),"Variable Decleration"),
        (new InputRust(),new InputGo(),new InputSharp(), new InputPython(), new InputPlus(),"Take User Input"),
        (new MathematicsRust(),new MathematicsGo(),new MathematicsSharp(), new MathematicsPython(), new MathematicsPlus(),"Mathematical Operations"),
        (new StringRust(),new StringGo(),new StringSharp(), new StringPython(), new StringPlus(),"String"),
        (new IfElseRust(),new IfElseGo(),new IfElseSharp(), new IfElsePython(), new IfElsePlus(),"If Else"),
        (new LoopRust(),new LoopGo(),new LoopSharp(), new LoopPython(), new LoopPlus(),"Loop"),
        (new LinearRust(),new LinearGo(),new LinearSharp(), new LinearPython(), new LinearPlus(),"Linear Data Types"),
        (new FunctionRust(),new FunctionGo(),new FunctionSharp(), new FunctionPython(), new FunctionPlus(),"Function"),
        (new DictRust(),new DictGo(),new DictSharp(), new DictPython(), new DictPlus(),"Dictionary and Hashset"),
        (new ClassRust(),new ClassGo(),new ClassSharp(), new ClassPython(), new ClassPlus(),"Class"),
        (new InterfaceRust(),new InterfaceGo(),new InterfaceSharp(), new InterfacePython(), new InterfacePlus(),"Interface And Inheritance")
        };
    private RenderFragment RenderComponent(ComponentBase component)
    {
        RenderFragment fragment = builder =>
        {
            builder.OpenComponent(0, component.GetType());
            builder.CloseComponent();
        };

        return fragment;
    }
}
